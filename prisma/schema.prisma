generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Customer {
  id    Int    @id @default(autoincrement())
  name  String
  email String

  @@map("customers")
}

model PriceHistory {
  id        Int    @id @default(autoincrement())
  symbol    String
  price     Float
  timestamp BigInt

  @@map("price_history")
}
